{
  "name": "node-starter-project-full",
  "version": "0.1.0",
  "description": "Starter Project For Node (With VueJS & KoaJS)",
  "main": "dist/index.js",
  "repository": {
    "private": true
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "/*************start-scripts*************/": "",
    "start": "nodemon dist/index.js --ignore dist/ res/ static/ --exec 'npm run build && node'",
    "server": "npm start",
    "admin": "sm-webpack serve --config=admin",
    "basic": "sm-webpack serve --config=basic",
    "dev": "sm-webpack serve --config=basic",
    "/*************tests-and-lint*************/": "",
    "test": "npm run lint && npm run test-coverage",
    "check-coverage": "true",
    "lint": "eslint src res test --ext .js --ext .vue --ext .html",
    "lint:fix": "npm run lint -- --fix",
    "test-only": "npm run build && NODE_ENV=test mocha --exit --require @babel/register $(find test -iname *.test.js)",
    "test-coverage": "npm run build && NODE_ENV=test nyc mocha --exit --require @babel/register $(find test -iname *.test.js)",
    "/*************build-scripts*************/": "",
    "build": "babel-changed",
    "build:admin": "sm-webpack --config=admin",
    "build:basic": "sm-webpack --config=basic",
    "/*************db-scripts*************/": "",
    "migrate": "knex migrate:latest",
    "migrate:create": "knex migrate:make",
    "refresh": "knex-utils refresh",
    "seed": "knex seed:run",
    "/*************other-scripts*************/": "",
    "logs": "oak-viewer --path $(cfg get logstashDir)/logs",
    "precommit": "npm run build && npm test && npm run check-coverage"
  },
  "author": "Hitesh Khandelwal <hitesh@smartprix.com> (http://www.smartprix.com/)",
  "license": "ISC",
  "dependencies": {
    "@smpx/oak": "^1.2.5",
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "el-admin": "^0.4.2",
    "element-ui": "^2.4.11",
    "gqutils": "^1.2.0",
    "graphql": "^14.0.2",
    "graphql-server-koa": "^1.4.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.3",
    "knex": "^0.15.2",
    "koa": "^2.6.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static-cache": "^5.1.2",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mysql2": "^1.6.4",
    "nunjucks": "^3.1.4",
    "objection": "^1.4.0",
    "pg": "^7.6.1",
    "sm-utils": "^2.15.11",
    "ua-parser-js": "^0.7.19",
    "urijs": "^1.19.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "vutils": "^0.14.20",
    "xorm": "^0.6.44"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@smpx/babel-changed": "^0.2.0",
    "@smpx/knex-utils": "^1.0.2",
    "@types/bluebird-global": "^3.5.9",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-smartprix": "^3.5.4",
    "husky": "^1.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sm-webpack-config": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "smartprix",
    "globals": {
      "d": false,
      "_": false,
      "moment": false
    },
    "rules": {
      "no-await-in-loop": 0,
      "no-console": 0,
      "vue/script-indent": 0,
      "vue/order-in-components": 0,
      "vue/attributes-order": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "modules": "commonjs",
          "shippedProposals": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "sourceMaps": true
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "html",
      "text-summary",
      "json-summary"
    ],
    "include": [
      "src"
    ],
    "sourceMap": false,
    "instrument": true,
    "all": false,
    "cache": true
  }
}
