{
  "name": "node-starter-project-full",
  "version": "0.1.0",
  "description": "Starter Project For Node (With VueJS & KoaJS)",
  "main": "dist/index.js",
  "repository": {
    "private": true
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon src/index.js --ignore dist/ res/ static/ --exec babel-node",
    "dev": "node build.js",
    "test": "npm run lint && npm run test-only",
    "check-coverage": "true",
    "build": "node build.js build && babel src --out-dir dist",
    "lint": "eslint src res --ext .js --ext .vue --ext .html",
    "lint:fix": "eslint src res --ext .js --ext .vue --ext .html --fix",
    "test-only": "NODE_ENV=test mocha --exit --require babel-register test/*.test.js",
    "migrate": "knex migrate:latest",
    "migrate:create": "knex migrate:make",
    "server": "npm start",
    "basic": "node build.js run-basic",
    "admin": "node build.js run-admin",
    "precommit": "npm run build && npm test && npm run check-coverage",
    "seed": "knex seed:run",
    "refresh": "node build.js refresh-db"
  },
  "author": "Hitesh Khandelwal <hitesh@smartprix.com> (http://www.smartprix.com/)",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "el-admin": "^0.3.6",
    "element-ui": "^2.3.3",
    "gqutils": "^1.1.0",
    "graphql": "0.13.2",
    "graphql-server-koa": "^1.3.4",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "2.24.0",
    "knex": "^0.14.5",
    "koa": "2.5.0",
    "koa-body": "2",
    "koa-router": "7.4.0",
    "koa-send": "^4.1.3",
    "koa-static-cache": "5.1.2",
    "koa-views": "6.1.4",
    "lodash": "4.17.5",
    "moment": "^2.22.0",
    "mysql2": "^1.5.3",
    "objection": "^1.1.3",
    "sm-utils": "^2.13.10",
    "vue": "2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "3.0.1",
    "vutils": "^0.14.7",
    "xorm": "^0.6.36",
    "pg": "^7.4.1",
    "ua-parser-js": "^0.7.17",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-register": "6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-smartprix": "^3.4.2",
    "husky": "^0.14.3",
    "mocha": "^5.0.5",
    "sm-webpack-config": "^0.18.2"
  },
  "eslintConfig": {
    "extends": "smartprix",
    "globals": {
      "d": false,
      "_": false,
      "moment": false
    },
    "rules": {
      "no-await-in-loop": 0,
      "no-console": 0,
      "vue/script-indent": 0,
      "vue/order-in-components": 0,
      "vue/attributes-order": 0
    }
  },
  "babel": {
    "plugins": [
      "transform-export-extensions",
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-es2015-modules-commonjs"
    ],
    "sourceMaps": true
  }
}
